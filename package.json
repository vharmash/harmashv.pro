{
  "name": "harmashv.pro",
  "version": "1.0.3",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-s lint format type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc  --build --force",
    "lint": "eslint . --fix -c eslint.config.js",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run tests/e2e/*.spec.ts",
    "test:unit": "vitest run tests/unit/**/*.spec.ts"
  },
  "peerDependencyRules": {
    "allowedVersions": {
      "eslint": "9"
    }
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@rushstack/eslint-patch": "^1.13.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "unplugin-inject-preload": "^3.0.0",
    "vite": "^7.1.9",
    "vite-imagetools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.12"
  }
}
